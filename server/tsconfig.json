{
  "compilerOptions": {
    "lib": ["es2020"],
    "module": "commonjs",
    "target": "ES2020",

    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "dist"
  }
}
